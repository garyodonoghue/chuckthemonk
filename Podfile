# platform :ios, '9.0'

target 'chuckthemonk' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks

  # Pods for chuckthemonk
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'Alamofire', '~> 5.0.0.beta.1'   # Networking
  pod 'SwiftyJSON'                     # JSON parsing
  pod 'HockeySDK', '~> 5.1.4'          # HockeyApp (private logging)
  
  target 'chuckthemonkTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'chuckthemonkUITests' do
    use_frameworks!
    inherit! :search_paths
    pod 'iOSSnapshotTestCase'
    # Pods for testing
  end

end
